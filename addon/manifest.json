{
  "manifest_version": 2,
  "name": "IPFS cached",
  "version": "0.1.0",

  "description": "Cache Web-Requests via IPFS",

  "icons": {
    "48": "icons/border-48.png"
  },

  "content_security_policy": "default-src 'self' wss://*; object-src https://example.com/; script-src 'self' 'unsafe-eval';",
  "web_accessible_resources": [
  ],
  "permissions": [
    "*://*/*",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "proxy",
    "storage"
  ],

  "background": {
    "scripts": ["background.js"]
  }
}